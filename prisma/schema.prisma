generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Agendamento {
  id             Int      @id @default(autoincrement())
  nome           String
  dataNascimento DateTime
  dataHora       DateTime
  realizado      Boolean  @default(false)

  //Deixa a busca mais rapdia
  @@index([dataHora])
}
